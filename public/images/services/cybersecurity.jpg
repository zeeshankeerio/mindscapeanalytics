<svg width="1200" height="800" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#2c3e50" />
      <stop offset="100%" stop-color="#1a2530" />
    </linearGradient>
    <linearGradient id="shield-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#27ae60" />
      <stop offset="100%" stop-color="#2ecc71" />
    </linearGradient>
    <radialGradient id="glow" cx="50%" cy="50%" r="50%" fx="50%" fy="50%">
      <stop offset="0%" stop-color="#27ae60" stop-opacity="0.3" />
      <stop offset="100%" stop-color="#27ae60" stop-opacity="0" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="100%" height="100%" fill="url(#bg)" />
  
  <!-- Digital circuit pattern -->
  <g opacity="0.1">
    <path d="M50 100 H400 V200 H600 V400 H800 V600 H1000 V700 H1150 M1150 300 H800 V500 H600 V300 H400 V100 M200 700 V500 H400 V400 H200 V200 H400" 
          stroke="#ffffff" stroke-width="3" fill="none" />
    <circle cx="400" cy="100" r="8" fill="#ffffff" />
    <circle cx="600" cy="200" r="8" fill="#ffffff" />
    <circle cx="800" cy="400" r="8" fill="#ffffff" />
    <circle cx="1000" cy="600" r="8" fill="#ffffff" />
    <circle cx="800" cy="500" r="8" fill="#ffffff" />
    <circle cx="600" cy="300" r="8" fill="#ffffff" />
    <circle cx="400" cy="400" r="8" fill="#ffffff" />
    <circle cx="200" cy="500" r="8" fill="#ffffff" />
    <circle cx="200" cy="200" r="8" fill="#ffffff" />
  </g>
  
  <!-- Binary code background -->
  <g fill="#ffffff" opacity="0.05" font-family="Courier New" font-size="12">
    <text x="50" y="100">01001001 01101110 01100110 01101111 01110011 01100101 01100011</text>
    <text x="50" y="120">01010000 01110010 01101111 01110100 01100101 01100011 01110100</text>
    <text x="50" y="140">01000011 01111001 01100010 01100101 01110010 01110011 01100101</text>
    <text x="50" y="160">01000100 01100001 01110100 01100001 01010011 01100001 01100110</text>
    <text x="50" y="180">01000110 01101001 01110010 01100101 01110111 01100001 01101100</text>
    <text x="50" y="200">01010011 01100101 01100011 01110101 01110010 01100101 01001001</text>
    
    <text x="800" y="500">01001001 01101110 01100110 01101111 01110011 01100101 01100011</text>
    <text x="800" y="520">01010000 01110010 01101111 01110100 01100101 01100011 01110100</text>
    <text x="800" y="540">01000011 01111001 01100010 01100101 01110010 01110011 01100101</text>
    <text x="800" y="560">01000100 01100001 01110100 01100001 01010011 01100001 01100110</text>
    <text x="800" y="580">01000110 01101001 01110010 01100101 01110111 01100001 01101100</text>
    <text x="800" y="600">01010011 01100101 01100011 01110101 01110010 01100101 01001001</text>
  </g>
  
  <!-- Main security shield -->
  <g transform="translate(600, 350)">
    <!-- Glow effect -->
    <circle cx="0" cy="0" r="250" fill="url(#glow)" />
    
    <!-- Shield outline -->
    <path d="M0,-200 C-120,-200 -180,-150 -180,-50 C-180,100 -100,200 0,250 C100,200 180,100 180,-50 C180,-150 120,-200 0,-200 Z" 
          fill="none" stroke="#27ae60" stroke-width="8" />
    
    <!-- Inner shield -->
    <path d="M0,-170 C-100,-170 -150,-130 -150,-50 C-150,80 -80,170 0,220 C80,170 150,80 150,-50 C150,-130 100,-170 0,-170 Z" 
          fill="url(#shield-gradient)" opacity="0.3" />
    
    <!-- Lock inside shield -->
    <rect x="-50" y="-30" width="100" height="80" rx="10" ry="10" fill="#27ae60" />
    <rect x="-60" y="-60" width="120" height="40" rx="5" ry="5" fill="none" stroke="#27ae60" stroke-width="12" />
    <circle cx="0" cy="10" r="15" fill="#1a2530" />
    <rect x="-5" y="0" width="10" height="25" rx="2" ry="2" fill="#1a2530" />
  </g>
  
  <!-- Security lock and key elements -->
  <g transform="translate(300, 250)">
    <circle cx="0" cy="0" r="80" fill="none" stroke="#ffffff" stroke-width="5" stroke-opacity="0.1" />
    <path d="M-30,-30 L30,30 M-30,30 L30,-30" stroke="#ffffff" stroke-width="5" stroke-opacity="0.1" />
  </g>
  
  <g transform="translate(900, 250)">
    <circle cx="0" cy="0" r="80" fill="none" stroke="#ffffff" stroke-width="5" stroke-opacity="0.1" />
    <path d="M-30,-30 L30,30 M-30,30 L30,-30" stroke="#ffffff" stroke-width="5" stroke-opacity="0.1" />
  </g>
  
  <!-- Animated lock scanning effect -->
  <g transform="translate(600, 350)">
    <circle cx="0" cy="0" r="200" fill="none" stroke="#27ae60" stroke-width="2" stroke-opacity="0.5">
      <animate attributeName="r" values="190;220;190" dur="4s" repeatCount="indefinite" />
      <animate attributeName="stroke-opacity" values="0.7;0.2;0.7" dur="4s" repeatCount="indefinite" />
    </circle>
    <circle cx="0" cy="0" r="220" fill="none" stroke="#27ae60" stroke-width="1" stroke-opacity="0.3">
      <animate attributeName="r" values="220;250;220" dur="4s" repeatCount="indefinite" />
      <animate attributeName="stroke-opacity" values="0.5;0.1;0.5" dur="4s" repeatCount="indefinite" />
    </circle>
  </g>
  
  <!-- Text label -->
  <text x="600" y="700" font-family="Arial" font-size="40" font-weight="bold" text-anchor="middle" fill="white">Cybersecurity Services</text>
</svg> 