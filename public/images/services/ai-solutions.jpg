<svg width="1200" height="800" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#2d1f3d" />
      <stop offset="100%" stop-color="#1b1129" />
    </linearGradient>
    <linearGradient id="highlight" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#8e44ad" />
      <stop offset="100%" stop-color="#9b59b6" />
    </linearGradient>
    <radialGradient id="glow" cx="50%" cy="50%" r="50%" fx="50%" fy="50%">
      <stop offset="0%" stop-color="#8e44ad" stop-opacity="0.6" />
      <stop offset="100%" stop-color="#8e44ad" stop-opacity="0" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="100%" height="100%" fill="url(#bg)" />
  
  <!-- Grid pattern -->
  <path d="M50 50 H1150 M50 150 H1150 M50 250 H1150 M50 350 H1150 M50 450 H1150 M50 550 H1150 M50 650 H1150 M50 750 H1150 M150 50 V750 M250 50 V750 M350 50 V750 M450 50 V750 M550 50 V750 M650 50 V750 M750 50 V750 M850 50 V750 M950 50 V750 M1050 50 V750" stroke="#ffffff" stroke-width="1" stroke-opacity="0.05" />
  
  <!-- Neural network nodes and connections -->
  <g transform="translate(600, 400)">
    <!-- Glow effect -->
    <circle cx="0" cy="0" r="300" fill="url(#glow)" />
    
    <!-- Input layer -->
    <circle cx="-250" cy="-150" r="15" fill="#9b59b6" />
    <circle cx="-250" cy="-50" r="15" fill="#9b59b6" />
    <circle cx="-250" cy="50" r="15" fill="#9b59b6" />
    <circle cx="-250" cy="150" r="15" fill="#9b59b6" />
    
    <!-- Hidden layer 1 -->
    <circle cx="-100" cy="-180" r="15" fill="#3498db" />
    <circle cx="-100" cy="-90" r="15" fill="#3498db" />
    <circle cx="-100" cy="0" r="15" fill="#3498db" />
    <circle cx="-100" cy="90" r="15" fill="#3498db" />
    <circle cx="-100" cy="180" r="15" fill="#3498db" />
    
    <!-- Hidden layer 2 -->
    <circle cx="50" cy="-150" r="15" fill="#3498db" />
    <circle cx="50" cy="-75" r="15" fill="#3498db" />
    <circle cx="50" cy="0" r="15" fill="#3498db" />
    <circle cx="50" cy="75" r="15" fill="#3498db" />
    <circle cx="50" cy="150" r="15" fill="#3498db" />
    
    <!-- Output layer -->
    <circle cx="200" cy="-100" r="15" fill="#e74c3c" />
    <circle cx="200" cy="0" r="15" fill="#e74c3c" />
    <circle cx="200" cy="100" r="15" fill="#e74c3c" />
    
    <!-- Connections - Input to Hidden 1 -->
    <line x1="-250" y1="-150" x2="-100" y2="-180" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-250" y1="-150" x2="-100" y2="-90" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-250" y1="-150" x2="-100" y2="0" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-250" y1="-50" x2="-100" y2="-180" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-250" y1="-50" x2="-100" y2="-90" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-250" y1="-50" x2="-100" y2="0" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-250" y1="-50" x2="-100" y2="90" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-250" y1="50" x2="-100" y2="-90" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-250" y1="50" x2="-100" y2="0" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-250" y1="50" x2="-100" y2="90" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-250" y1="50" x2="-100" y2="180" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-250" y1="150" x2="-100" y2="0" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-250" y1="150" x2="-100" y2="90" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-250" y1="150" x2="-100" y2="180" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    
    <!-- Connections - Hidden 1 to Hidden 2 -->
    <line x1="-100" y1="-180" x2="50" y2="-150" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-100" y1="-180" x2="50" y2="-75" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-100" y1="-90" x2="50" y2="-150" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-100" y1="-90" x2="50" y2="-75" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-100" y1="-90" x2="50" y2="0" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-100" y1="0" x2="50" y2="-75" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-100" y1="0" x2="50" y2="0" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-100" y1="0" x2="50" y2="75" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-100" y1="90" x2="50" y2="0" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-100" y1="90" x2="50" y2="75" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-100" y1="90" x2="50" y2="150" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-100" y1="180" x2="50" y2="75" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="-100" y1="180" x2="50" y2="150" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    
    <!-- Connections - Hidden 2 to Output -->
    <line x1="50" y1="-150" x2="200" y2="-100" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="50" y1="-75" x2="200" y2="-100" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="50" y1="-75" x2="200" y2="0" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="50" y1="0" x2="200" y2="-100" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="50" y1="0" x2="200" y2="0" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="50" y1="0" x2="200" y2="100" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="50" y1="75" x2="200" y2="0" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="50" y1="75" x2="200" y2="100" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="50" y1="150" x2="200" y2="0" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    <line x1="50" y1="150" x2="200" y2="100" stroke="#ffffff" stroke-width="2" stroke-opacity="0.2" />
    
    <!-- Animated data flow -->
    <circle cx="-170" cy="-120" r="3" fill="#9b59b6">
      <animate attributeName="cx" values="-250;-100" dur="2s" repeatCount="indefinite" />
      <animate attributeName="cy" values="-150;-90" dur="2s" repeatCount="indefinite" />
    </circle>
    <circle cx="-180" cy="20" r="3" fill="#9b59b6">
      <animate attributeName="cx" values="-250;-100" dur="1.8s" repeatCount="indefinite" />
      <animate attributeName="cy" values="50;0" dur="1.8s" repeatCount="indefinite" />
    </circle>
    <circle cx="-20" cy="-100" r="3" fill="#3498db">
      <animate attributeName="cx" values="-100;50" dur="1.6s" repeatCount="indefinite" />
      <animate attributeName="cy" values="-90;-75" dur="1.6s" repeatCount="indefinite" />
    </circle>
    <circle cx="0" cy="40" r="3" fill="#3498db">
      <animate attributeName="cx" values="-100;50" dur="2.2s" repeatCount="indefinite" />
      <animate attributeName="cy" values="0;0" dur="2.2s" repeatCount="indefinite" />
    </circle>
    <circle cx="120" cy="-50" r="3" fill="#e74c3c">
      <animate attributeName="cx" values="50;200" dur="1.5s" repeatCount="indefinite" />
      <animate attributeName="cy" values="-75;-100" dur="1.5s" repeatCount="indefinite" />
    </circle>
    <circle cx="120" cy="60" r="3" fill="#e74c3c">
      <animate attributeName="cx" values="50;200" dur="2.1s" repeatCount="indefinite" />
      <animate attributeName="cy" values="75;100" dur="2.1s" repeatCount="indefinite" />
    </circle>
  </g>
  
  <!-- Brain icon -->
  <g transform="translate(350, 200) scale(0.7)">
    <ellipse cx="0" cy="0" rx="120" ry="150" fill="none" stroke="#8e44ad" stroke-width="6" />
    <path d="M-60,-40 C-30,-80 30,-80 60,-40" fill="none" stroke="#8e44ad" stroke-width="5" />
    <path d="M-90,0 C-60,-40 60,-40 90,0" fill="none" stroke="#8e44ad" stroke-width="5" />
    <path d="M-90,40 C-60,0 60,0 90,40" fill="none" stroke="#8e44ad" stroke-width="5" />
    <path d="M-60,80 C-30,40 30,40 60,80" fill="none" stroke="#8e44ad" stroke-width="5" />
    <path d="M0,-80 L0,80" fill="none" stroke="#8e44ad" stroke-width="5" />
  </g>
  
  <!-- Text label -->
  <text x="600" y="700" font-family="Arial" font-size="40" font-weight="bold" text-anchor="middle" fill="white" opacity="0.9">Generative AI Solutions</text>
</svg> 