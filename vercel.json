{
  "version": 2,
  "buildCommand": "pnpm install --no-frozen-lockfile && npx prisma generate && pnpm build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "framework": "nextjs",
  "devCommand": "pnpm dev",
  "regions": ["iad1"],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index" }
  ]
} 